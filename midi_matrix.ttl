@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .

# Maintainer
<http://open-music-kontrollers.ch#me>
	a foaf:Person ;
	foaf:name "Hanspeter Portner" ;
	foaf:mbox <dev@open-music-kontrollers.ch> ;
	foaf:homepage <http://open-music-kontrollers.ch> .

# Project
<http://open-music-kontrollers.ch/lv2/midi_matrix>
	a doap:Project ;
	doap:maintainer <http://open-music-kontrollers.ch#me> ;
	doap:name "Various MIDI Matrix Multiplexers and Demultiplexers" .

# Plugin UI
<http://open-music-kontrollers.ch/lv2/midi_matrix#channel_filter/ui>
	a ui:X11UI ;
	lv2:requiredFeature ui:idleInterface ;
  lv2:optionalFeature ui:noUserResize ;
  lv2:extensionData ui:idleInterface, ui:showInterface .

# Plugin
<http://open-music-kontrollers.ch/lv2/midi_matrix#channel_filter>
	a lv2:Plugin,
		lv2:MixerPlugin;
	doap:name "MIDI Channel Matrix Multiplexer and Demultiplexer" ;
	doap:license <http://opensource.org/licenses/isc> ;
	lv2:project <http://open-music-kontrollers.ch/lv2/midi_matrix> ;
	lv2:optionalFeature lv2:isLive, lv2:hardRTCapable ;
	lv2:requiredFeature urid:map ;

	lv2:port [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "mask_channel_1" ;
		lv2:name "Mask of Channel 1" ;
		lv2:default 1.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 65535.0 ;
		lv2:portProperty lv2:integer ;
	] ;

	lv2:port [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 1 ;
