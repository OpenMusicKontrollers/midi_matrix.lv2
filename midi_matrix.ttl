		lv2:name "Mask of Channel 11" ;
		lv2:default 1024 ;
		lv2:minimum 0 ;
		lv2:maximum 65535 ;
		lv2:portProperty lv2:integer, lv2:isBitmask ;
	] , [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 11 ;
		lv2:symbol "mask_channel_12" ;
