	] , [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 14 ;
		lv2:symbol "mask_channel_15" ;
		lv2:name "Mask of Channel 15" ;
		lv2:default 16384 ;
		lv2:minimum 0 ;
