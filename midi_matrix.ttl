		lv2:portProperty lv2:integer, lv2:isBitmask ;
	] , [
	  a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		lv2:index 16 ;
		lv2:symbol "midi_in" ;
		lv2:name "MIDI Input Port" ;
		lv2:designation lv2:control ;
	] , [
