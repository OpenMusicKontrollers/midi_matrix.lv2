@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix owl:	<http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:   <http://lv2plug.in/ns/extensions/ui#> .
@prefix kx:		<http://kxstudio.sf.net/ns/lv2ext/external-ui#> .

@prefix midi_matrix:	<http://open-music-kontrollers.ch/lv2/midi_matrix#> .

# to please sord_validate
ui:EoUI
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf ui:UI .
kx:Widget
	a rdfs:Class, owl:Class ;
	rdfs:subClassOf ui:UI .
kx:Host
	a lv2:Feature .

# Channel Filter Plugin
midi_matrix:channel_filter
	a lv2:Plugin ;
	lv2:minorVersion @MIDI_MATRIX_MINOR_VERSION@ ;
	lv2:microVersion @MIDI_MATRIX_MICRO_VERSION@ ;
	lv2:binary <midi_matrix@CMAKE_SHARED_MODULE_SUFFIX@> ;
	ui:ui midi_matrix:channel_filter_1_ui ;
	ui:ui midi_matrix:channel_filter_2_kx ;
	ui:ui midi_matrix:channel_filter_3_eo ;
	rdfs:seeAlso <midi_matrix.ttl> .

midi_matrix:channel_filter_1_ui
	a ui:UI ;
	ui:binary <midi_matrix_ui@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <midi_matrix_ui.ttl> .
midi_matrix:channel_filter_2_kx
	a kx:Widget ;
	ui:binary <midi_matrix_ui@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <midi_matrix_ui.ttl> .
midi_matrix:channel_filter_3_eo
	a ui:EoUI ;
	ui:binary <midi_matrix_eo@CMAKE_SHARED_MODULE_SUFFIX@> ;
	rdfs:seeAlso <midi_matrix_ui.ttl> .
